{
  "name": "@react-force/web-utils",
  "description": "Web API utilities",
  "version": "2.1.0",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nareshbhatia/react-force.git",
    "directory": "packages/web-utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/mock-data.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "build:local": "yarn lint && yarn test:coverage && yarn build",
    "del-dist": "rimraf dist",
    "format": "prettier --write '{src,test,stories}/**/*.ts*'",
    "lint": "eslint '{src,test,stories}/**/*.ts*'",
    "prepublish": "yarn build",
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "test": "echo No tests in this package",
    "test:watch": "echo No tests in this package",
    "test:coverage": "echo No tests in this package"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5"
  }
}
